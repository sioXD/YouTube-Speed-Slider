{
  "manifest_version": 3,
  "name": "YouTube Speed Control Slider",
  "description": "Add playback speed Slider and controls to the YouTube video player controls.",
  "version": "1.6.0",
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"],
      "all_frames": true
    }
  ],
  "options_ui": {
    "page": "pages/options.html",
    "open_in_tab": false
  },
  "permissions": [
    "storage"
  ]
}